<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.XiangmuDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.XiangmuEntity" id="xiangmuMap">
        <result property="xiangmumingcheng" column="xiangmumingcheng"/>
        <result property="shigongdanwei" column="shigongdanwei"/>
        <result property="yongdimianji" column="yongdimianji"/>
        <result property="xiangmuleixing" column="xiangmuleixing"/>
        <result property="xiangmujingli" column="xiangmujingli"/>
        <result property="xiangmukaishishijian" column="xiangmukaishishijian"/>
        <result property="xiangmujieshushijian" column="xiangmujieshushijian"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.XiangmuVO" >
		SELECT * FROM xiangmu  xiangmu         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.XiangmuVO" >
		SELECT  xiangmu.* FROM xiangmu  xiangmu
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.XiangmuView" >

		SELECT  xiangmu.* FROM xiangmu  xiangmu 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.XiangmuView" >
		SELECT * FROM xiangmu  xiangmu <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	
</mapper>